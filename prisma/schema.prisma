generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuarios {
  idusuario Int    @id @default(autoincrement())
  nombre    String @db.VarChar(200) @unique
  email     String @db.VarChar(200) @unique
  password  String @db.VarChar(200)
}
